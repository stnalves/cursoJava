package exercicios;

import java.util.Scanner;

public class QuadradoCubo {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int n = sc.nextInt();
		
		for (int i = 1; i <= n; i++) {
			System.out.printf("%d %d %d", i, i * i, Math.pow(i, 3));
		}
		
		sc.close();
	}
}
